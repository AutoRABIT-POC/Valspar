        <externalId>false</externalId>
        <formula>if (Current_FY_Revenue_Override__c &lt;&gt; 0,
Current_FY_Revenue_Override__c,
if (Closed_In_Prior_FY__c = 1,
Annual_Expected_Revenue__c - Prior_FY_Full_Revenue__c ,
if (Closed_In_Current_FY__c = 1,
Close_Year_Full_Revenue__c,
0)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Current FY Full Revenue</label>
        <precision>18</precision>
