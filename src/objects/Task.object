    <validationRules>
        <fullName>Duration_in_Minutes_Curriculum</fullName>
        <active>true</active>
        <description>Ensures that the duration in minutes is set to 0 if training curriculum is selected</description>
        <errorConditionFormula>ISPICKVAL(Activity_Type__c, &apos;Training Curriculum&apos;) &amp;&amp; Duration_in_Minutes__c  &lt;&gt; 0</errorConditionFormula>
        <errorMessage>The duration in minutes for a training curriculum must be zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Training_Type_Required</fullName>
        <active>true</active>
        <description>Makes the training type field required if the selected Activity Type is Training Course or Training Curriculum</description>
        <errorConditionFormula>(ISPICKVAL( Activity_Type__c , &apos;Training Course&apos;) || ISPICKVAL( Activity_Type__c , &apos;Training Curriculum&apos;)) &amp;&amp; ISPICKVAL( Training_Type__c,&apos;&apos;)</errorConditionFormula>
        <errorMessage>You must select a training type</errorMessage>
    </validationRules>
